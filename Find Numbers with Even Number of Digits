public class Main {

    public static void main(String[] args) {
        int[] arr = {10,15,22,0};
         System.out.println(numbersWithEvenDigits(arr));
    }
    
  public static int numbersWithEvenDigits(int[] nums) {

        int numberswithEvenDigits = 0; // count the numbers with even digits
        for (int i=0;i<nums.length;i++) {
            if (String.valueOf(nums[i]).length() % 2 == 0) {
                numberswithEvenDigits++;

            }
        }
        return numberswithEvenDigits;

    }

}



