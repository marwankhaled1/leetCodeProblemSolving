public class Main {
    public static void main(String[] args) {

        stairCase(6);
    }


    public static void stairCase(int n)
    {
        for(int i=1;i<=n;i++)
        {

            for(int j=n-i;j>0;j--){

                System.out.print(" ");
            }

            for(int x=n-i;x<n;x++)
            {

                System.out.print("#");
            }
            System.out.println();

        }



    }
}
